@model IEnumerable<EventsPlus.Models.Organizer>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrganizerLastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrganizerFirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrganizerContactNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrganizerEmailAddress)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrganizerLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrganizerFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrganizerContactNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrganizerEmailAddress)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
